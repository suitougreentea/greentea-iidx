body
  padding-top 75px

// Prefix for original class
.gt-
  &table-musiclist
    thead > tr > th, tbody > tr > td
      height 24px
      padding 0px 8px
      vertical-align middle
    .form-control
      height 20px
      padding 0px 6px
    .btn
      width 120px
      padding 0px 6px
  &nowrap
    white-space nowrap
  &scoreinput
    text-align right
    width 64px
  &difficultylabel
    font-size: 80%;
    width: 100px;
    &-small
      width 24px
    &-normal
      background-color #05C
    &-hyper
      background-color #B90
    &-another
      background-color #C00